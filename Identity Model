    <Description>
       Initialize the data for the identity that we are creating.
    </Description>
    <Transition to="xyz"/>
 </Step> 

  <Script>
      <Source>
        String name = (String)identityModel.get("name");
        if (name != null) {
          name = name.trim();
        }
        return name;
      </Source>
